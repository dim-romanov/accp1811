		<xs:attribute name="REMARKS" type="REMARKS_Type" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="RUS">Текст примечания к файлу</xs:documentation>
				<xs:documentation xml:lang="ENG">Additional remarks</xs:documentation>
			</xs:annotation>
			<!--Примечание к документу-->
		</xs:attribute>
		<xs:attribute name="SIGNAUTHOR" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="RUS">ФИО автора ЭЦП</xs:documentation>
				<xs:documentation xml:lang="ENG">Name of the electronic signature author</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:element name="MICEX_DOC">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Корневой элемент (root element) XML документа</xs:documentation>
			<xs:documentation xml:lang="ENG">Document root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DOC_REQUISITES"/>
				<xs:element ref="CCX84"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="DOC_REQUISITES" type="DOC_REQUISITES_Type">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок информации о документе</xs:documentation>
			<xs:documentation xml:lang="ENG">Document metadata</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="CCX84">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок данных отчета</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with report data</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="SETTLE" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="ReportDate" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Дата, за которую формируется отчет</xs:documentation>
					<xs:documentation xml:lang="ENG">Report date</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ClearingFirmId" type="FIRMID_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Идентификатор Участника клиринга</xs:documentation>
					<xs:documentation xml:lang="ENG">Clearing Member ID</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ClearingFirmName" type="FIRMNAME_LONG_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование Участника клиринга</xs:documentation>
					<xs:documentation xml:lang="ENG">Clearing Member name</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ClearingFirmNameEN" type="FIRMNAME_LONG_Type" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование Участника клиринга (английское)</xs:documentation>
					<xs:documentation xml:lang="ENG">Clearing Member name (eng)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SETTLEDATE">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок данных по дате исполнения</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with data per settlement date</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence/>
			<xs:attribute name="SettleDate" type="xs:date" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Дата исполнения</xs:documentation>
					<xs:documentation xml:lang="ENG">Settlement date</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SETTLE">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок информации по расчетному коду</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with data per settlement code</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="RECORDS" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="ExtSettleCode" type="EXTSETTLECODE_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Расчетный код Участника клиринга</xs:documentation>
					<xs:documentation xml:lang="ENG">Clearing Member settlement code</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SumValue" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Общая стоимость Обеспечения в рублях</xs:documentation>
					<xs:documentation xml:lang="ENG">Full collateral value in rubles</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LockValue" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Стоимость Обеспечения для целей расчета норматива текущей ликвидности банка в рублях</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value in rubles for calculation current liquidity ratio purposes </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FreeValue" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Стоимость Обеспечения для целей расчета норматива мгновенной ликвидности банка в рублях</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value in rubles for calculation instant liquidity ratio purposes </xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SingleLimit" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Единый лимит</xs:documentation>
					<xs:documentation xml:lang="ENG">Single limit</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="FIRM">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок информации по Участнику торгов</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with data per Trading Member</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="CURRPAIR" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="FirmId" type="FIRMID_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Регистрационный код Участника торгов</xs:documentation>
					<xs:documentation xml:lang="ENG">Registration code of the Trading Member</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FirmName" type="FIRMNAME_LONG_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование Участника торгов</xs:documentation>
					<xs:documentation xml:lang="ENG">Trading Member name</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FirmNameEN" type="FIRMNAME_LONG_Type" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование Участника торгов (английское)</xs:documentation>
					<xs:documentation xml:lang="ENG">Trading Member name (eng)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="CURRPAIR">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок данных валютной пары</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with data per currency pair</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence/>
			<xs:attribute name="CoCurrencyId" type="CURRENCYID_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Идентификатор сопряженной валюты</xs:documentation>
					<xs:documentation xml:lang="ENG">Quote currency ID</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CoCurrencyName" type="CURRENCYNAME_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование сопряженной валюты</xs:documentation>
					<xs:documentation xml:lang="ENG">Quote currency name</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CoCurrencyNameEN" type="CURRENCYNAME_Type" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование сопряженной валюты (английское)</xs:documentation>
					<xs:documentation xml:lang="ENG">Quote currency name (eng)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="RECORDS">
		<xs:annotation>
			<xs:documentation xml:lang="RUS">Блок данных по сделке</xs:documentation>
			<xs:documentation xml:lang="ENG">Element with data per trade</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:attribute name="CurrencyId" type="CURRENCYID_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Идентификатор валюты/драгоценного металла</xs:documentation>
					<xs:documentation xml:lang="ENG">Currency/Precious metal ID</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyName" type="CURRENCYNAME_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование валюты/драгоценного металла</xs:documentation>
					<xs:documentation xml:lang="ENG">Currency/Precious metal name</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="CurrencyNameEN" type="CURRENCYNAME_Type" use="optional">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Наименование валюты/драгоценного металла (английское)</xs:documentation>
					<xs:documentation xml:lang="ENG">Currency/Precious metal name (eng)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Quantity" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Размер Обеспечения (в единицах валюты /в граммах)</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value (in currency units / grams)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="LockQuantity" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Размер Обеспечения для целей расчета норматива текущей ликвидности банка (в единицах валюты /в граммах)</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value for calculation current liquidity ratio purposes (in currency units / grams)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="FreeQuantity" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Размер Обеспечения для целей расчета норматива мгновенной ликвидности банка (в единицах валюты /в граммах)</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value for calculation instant liquidity ratio purposes  (in currency units / grams)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Value" type="FLOAT_20.2_Type" use="required">
				<xs:annotation>
					<xs:documentation xml:lang="RUS">Стоимость Обеспечения в рублях</xs:documentation>
					<xs:documentation xml:lang="ENG">Collateral value in rubles</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
</xs:schema>
